package com.company;

import com.sun.jdi.Value;

import java.util.*;

public class Main
{
    public static void main(String[] args)
    {
        Scanner in =new Scanner(System.in);
        System.out.print("Enter the Sentence: ");
        String string =in.nextLine();
        String string1[]=string.split(" ");
        for(String k:string1)
            System.out.print(k);
    }
}







